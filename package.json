{
  "name": "tedir-calendar",
  "version": "0.0.1-beta-1",
  "description": "Tedir Calendar is a Vue 3 & Web Component Event Calendar",
  "main": "dist/vue/tedir-calendar-vue.umd.js",
  "module": "dist/vue/tedir-calendar-vue.es.js",
  "unpkg": "dist/vue/tedir-calendar-vue.umd.js",
  "exports": {
    ".": {
      "import": "./dist/vue/tedir-calendar-vue.es.js",
      "require": "./dist/vue/tedir-calendar-vue.umd.js"
    },
    "./web": {
      "import": "./dist/web/tedir-calendar-web.es.js",
      "require": "./dist/web/tedir-calendar-web.umd.js"
    },
    "./css": "./dist/vue/style.css"
  },
  "scripts": {
    "dev-vue": "vite --config vite-vue-config.ts",
    "build-vue": "vue-tsc --noEmit && vite build --config vite-vue-config.ts",
    "build-web": "vue-tsc --noEmit && vite build --config vite-web-config.ts",
    "preview-vue": "vite preview --config vite-vue-config.ts"
  },
  "files": [
    "src/alga/*.alga",
    "package.alga",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tedirghazali/calendar.git"
  },
  "keywords": [
    "calendar",
    "event-calendar",
    "event",
    "vue3calendar",
    "datepicker",
    "timepicker",
    "datetimepicker",
    "daterangepicker"
  ],
  "author": "Tedir Ghazali",
  "license": "SEE LICENSE IN license.txt",
  "bugs": {
    "url": "https://github.com/tedirghazali/calendar/issues"
  },
  "homepage": "https://github.com/tedirghazali/calendar#readme",
  "devDependencies": {
    "@babel/types": "^7.20.5",
    "@vitejs/plugin-vue": "^4.0.0",
    "alga-css": "^1.0.0-beta-17",
    "alga-js": "^0.1.0-alpha-5",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  },
  "dependencies": {
    "vue": "^3.2.45"
  }
}
